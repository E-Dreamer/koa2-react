{
  "name": "koa-react",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "prepare": "husky install",
    "fix": "eslint --fix --ext .js,.ts,.tsx ./src",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "models": "cross-env NODE_ENV=development ts-node-dev --respawn ./server/config/auto.ts",
    "back:dev": "cross-env NODE_ENV=development ts-node-dev --respawn ./server/index.ts",
    "back:prod": "cross-env NODE_ENV=production ts-node-dev --respawn ./server/index.ts"
  },
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "axios": "^1.2.1",
    "bcryptjs": "^2.4.3",
    "cross-env": "^7.0.3",
    "jsonwebtoken": "^8.5.1",
    "koa-body": "^6.0.1",
    "koa-jwt": "^4.0.3",
    "koa-session": "^6.2.0",
    "koa-static": "^5.0.0",
    "koa-swagger-decorator": "^1.8.6",
    "log4js": "^6.7.1",
    "mysql2": "^2.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.1",
    "sequelize": "^6.26.0",
    "sequelize-auto": "^0.8.8",
    "svg-captcha": "^1.4.0",
    "ts-node-dev": "^2.0.0",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.3.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/koa": "^2.13.5",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-session": "^5.10.6",
    "@types/koa-static": "^4.0.2",
    "@types/koa__cors": "^3.3.0",
    "@types/koa__router": "^12.0.0",
    "@types/node": "^18.11.12",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@vitejs/plugin-react": "^2.2.0",
    "commitlint": "^17.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.30.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.31.11",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "mysql": "^2.18.1",
    "sass": "^1.57.1",
    "typescript": "^4.9.4",
    "vite": "^3.2.3",
    "vite-plugin-eslint": "^1.8.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0 --fix ",
      "prettier --write"
    ]
  }
}
